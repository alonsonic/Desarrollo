<UserControl x:Class="AeiMobile.MenuControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot" Background="#FFF16527">
    	<Button x:Name="botonPerfil" Content="Mi Perfil" HorizontalAlignment="Left" Height="84" Margin="0,10,0,0" VerticalAlignment="Top" Width="479"/>
    	<Button x:Name="botonCarrito" Content="Carrito" HorizontalAlignment="Left" Height="84" Margin="10,99,-9,0" VerticalAlignment="Top" Width="479" Click="botonCarrito_Click"/>
    	<Button Content="Button" HorizontalAlignment="Left" Height="84" Margin="0,178,0,0" VerticalAlignment="Top" Width="479"/>
    	<Button Content="Button" HorizontalAlignment="Left" Height="84" Margin="0,262,0,0" VerticalAlignment="Top" Width="479"/>
    </Grid>
</UserControl>