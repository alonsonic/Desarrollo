<UserControl x:Class="AeiMobile.MenuControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}" d:DesignWidth="480" Height="365">
    
    <Grid x:Name="LayoutRoot" Background="#FFF16527">
    	<Button x:Name="botonPerfil" Content="Mi Perfil" HorizontalAlignment="Left" Height="84" Margin="0,99,0,0" VerticalAlignment="Top" Width="479" Click="botonPerfil_Click"/>
    	<Button x:Name="botonCarrito" Content="Carrito" HorizontalAlignment="Left" Height="84" Margin="0,183,0,0" VerticalAlignment="Top" Width="479" Click="botonCarrito_Click"/>
    	<Button x:Name="botonInicio" Content="Inicio" HorizontalAlignment="Left" Height="84" Margin="1,10,0,0" VerticalAlignment="Top" Width="479" Click="botonInicio_Click"/>
    	<Button x:Name="botonCerrar" Content="Cerrar Sesión" HorizontalAlignment="Left" Height="84" Margin="1,267,0,0" VerticalAlignment="Top" Width="479" Click="botonCerrar_Click"/>
    </Grid>
</UserControl>