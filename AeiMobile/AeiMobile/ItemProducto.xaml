<UserControl x:Class="AeiMobile.ItemProducto"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}" d:DesignWidth="480" Width="475" Height="67">
    
    <Grid x:Name="LayoutRoot" Width="475" Height="50" Background="#FFF16527">
    	<Button x:Name="boton" Content="Producto" HorizontalAlignment="Left" Height="72" Margin="-12,-11,-11,-11" VerticalAlignment="Top" Width="498" BorderBrush="Transparent" Click="boton_Click"/>
    </Grid>
</UserControl>